package dataBase;

public class Food extends Restaurant{
	
	private String foodIncluded;
	private String foodIngredients;
	
	
	//constructor
	public Food() {
		
	}
	
	//setters and getters
	public String getFoodIncluded() {
		return this.foodIncluded;
	}
	
	public void setFoodIncluded(String foodincluded) {
		this.foodIncluded = foodincluded;
	}
	
	public String getFoodIngredients() {
		return this.foodIngredients;
	}
	
	public void setFoodIndredients(String foodingredients) {
		this.foodIngredients = foodingredients;
	}

}
